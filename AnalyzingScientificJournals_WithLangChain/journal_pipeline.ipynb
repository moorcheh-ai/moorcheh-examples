      }
    },
    {
      "cell_type": "code",
      "source": [
        "documents = []\n",
        "doc_id = []\n",
        "\n",
        "for i, chunk in enumerate(chunks):\n",
        "    text = chunk.page_content.strip()\n",
        "    doc_id.append(f\"chunk_{i}\")\n",
        "    documents.append(Document(page_content=text, metadata={\"topic\": \"LLMs\"}))\n",
        "\n",
        "store.add_documents(documents=documents, ids=doc_id)"
      ],
