    {
      "cell_type": "code",
      "source": [
        "loader = PyPDFLoader(pdf_path)\n",
        "pages = loader.load()\n",
        "\n",
        "splitter = RecursiveCharacterTextSplitter(chunk_size=700, chunk_overlap=120)\n",
        "chunks = splitter.split_documents(pages)"
      ],
      "metadata": {
        "id": "sNJpyIwdNwns"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
